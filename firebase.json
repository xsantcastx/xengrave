{
  "hosting": {
    "public": "dist/xengrave/browser",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "cleanUrls": true,
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.csr.html"
      }
    ],
    "predeploy": [
      "npm run build"
    ]
  }
}
